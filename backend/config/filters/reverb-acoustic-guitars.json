  "priorities": [
    { "key": "price", "label": "Price" },
    { "key": "condition_display", "label": "Condition" },
    { "key": "year", "label": "Year" },
    { "key": "free_expedited_shipping", "label": "Free shipping" },
    { "key": "make", "label": "Make" }
