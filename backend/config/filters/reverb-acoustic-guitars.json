  "filters": [
    {
      "key": "make",
      "label": "Make",
      "type": "checkboxes",
      "path": "make",
      "options": [
        "Collings",
        "Martin",
        "Bourgeois",
        "Taylor",
        "Glarry",
        "Winzz",
        "J.N Guitars",
        "Unbranded",
        "Szzzp",
        "Glarner Guitars",
        "DoBa"
      ]
    },
    {
      "key": "model",
      "label": "Model",
      "type": "checkboxes",
      "path": "model",
      "options": [
        "Acoustic Guitars",
        "Acoustic Guitars - D1A T - Traditional Series",
        "Acoustic Guitars - 1956 D-28",
        "Acoustic Guitars - Country Boy OM - Professional Series",
        "Acoustic Guitars - 01 T - Traditional Series",
        "Acoustic Guitars - D-35 (2025)",
        "Acoustic Guitars - K24ce Builder's Edition"
      ]
    },
    {
      "key": "finish",
      "label": "Finish",
      "type": "checkboxes",
      "path": "finish",
      "options": [
        "Natural",
        "Indian Rosewood",
        "Satin",
        "Mahogany",
        "Phosphor Bronze"
      ]
    },
    {
      "key": "year",
      "label": "Year",
      "type": "range",
      "path": "year",
