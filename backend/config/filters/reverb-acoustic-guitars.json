  "display": {
    "currency": "USD",
    "unit": "guitar",
    "title_template": "{title}",
    "subtitle_template": "{make} {model}",
    "image_path": "photos[0]._links.large_crop.href",
    "empty_image": "/assets/octonotes.png",
    "metadata": [
      { "label": "Price", "path": "price.amount", "format": "currency", "currency": "USD" },
      { "label": "Condition", "path": "condition.display_name" },
      { "label": "Year", "path": "year" },
      { "label": "Finish", "path": "finish" },
      { "label": "Shop", "path": "shop_name" }
