    {
      "key": "condition_display",
      "label": "Condition",
      "type": "checkboxes",
      "path": "condition.display_name",
      "options": [
        "Brand New",
        "Mint",
        "Excellent",
        "Very Good",
        "Good",
        "Fair",
        "Poor",
        "Non Functioning"
      ]
    },
    {
      "key": "price",
      "label": "Price (USD)",
      "type": "range",
      "path": "price.amount",
      "min": 0,
      "max": 20000
    },
    {
      "key": "free_expedited_shipping",
      "label": "Free expedited shipping",
      "type": "boolean",
      "path": "shipping.free_expedited_shipping"
    }
  ],
  "sections": [
    { "title": "Basics", "filters": ["price", "condition_display", "year", "free_expedited_shipping"] },
    { "title": "Make & Model", "filters": ["make", "model", "finish"] },
    { "title": "Search", "filters": ["description"] }
