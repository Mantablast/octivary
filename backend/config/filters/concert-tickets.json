{
  "config_key": "concert-tickets",
  "category_key": "entertainment",
  "title": "Concert Tickets",
  "description": "Live music and tours.",
  "disclaimer": "",
  "datasets": {
    "primary": {
      "label": "Listings",
      "data_source": {
        "type": "external_api",
        "provider_key": "events_v1"
      }
    }
  },
  "preset_filters": {
    "event_type": "concert"
  },
