    { "key": "genre", "label": "Genre", "type": "select", "options": ["pop", "rock", "hip-hop", "electronic", "classical"] }
  ],
  "sections": [
    { "title": "Basics", "filters": ["price", "date"] },
    { "title": "Details", "filters": ["city", "genre"] }
  ],
  "priorities": [
    { "key": "price", "label": "Affordability" },
    { "key": "location", "label": "Location" },
    { "key": "date", "label": "Date" },
    { "key": "seat_quality", "label": "Seat quality" }
